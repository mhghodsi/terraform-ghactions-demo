# Example of buckets configuration
# Note: Bucket names will automatically be prefixed with the workspace name (e.g., dev_my-first-bucket)
buckets = {
  "my-first-bucket" = {
    create = true
    acl    = "private"
  },
  "my-second-bucket" = {
    create = true
    acl    = "private"
  },
  "my-third-bucket" = {
    create = false  # This bucket will NOT be created (or will be removed if it exists)
  }
}
